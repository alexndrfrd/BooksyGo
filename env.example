# ============================================
# BooksyGo Environment Configuration
# ============================================
# Copy this file to .env and fill in your values

# Node Environment
NODE_ENV=development

# Database
DATABASE_URL=postgresql://booksygo:booksygo_dev_password@localhost:5432/booksygo

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=booksygo_redis_password

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=15m

# Skyscanner API
SKYSCANNER_API_KEY=your-skyscanner-api-key

# Booking.com API
BOOKING_API_KEY=your-booking-api-key

# OpenAI
OPENAI_API_KEY=your-openai-api-key

# Stripe
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:4000/api

# See full configuration in docs/environment-variables.md

